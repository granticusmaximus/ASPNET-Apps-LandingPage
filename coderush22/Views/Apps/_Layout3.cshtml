<!DOCTYPE html>
<html lang="zxx" class="no-js">
<head>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Author Meta -->
    <meta name="author" content="colorlib">
    <!-- Meta Description -->
    <meta name="description" content="">
    <!-- Meta Keyword -->
    <meta name="keywords" content="">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>Apps</title>
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700" rel="stylesheet">
    <!--
    CSS
    ============================================= -->
    <link rel="stylesheet" href="~/template/apps/css/linearicons.css">
    <link rel="stylesheet" href="~/template/apps/css/owl.carousel.css">
    <link rel="stylesheet" href="~/template/apps/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/template/apps/css/nice-select.css">
    <link rel="stylesheet" href="~/template/apps/css/magnific-popup.css">
    <link rel="stylesheet" href="~/template/apps/css/bootstrap.css">
    <link rel="stylesheet" href="~/template/apps/css/main.css">
</head>
<body>
    <!-- Start banner Area -->
    <section class="generic-banner relative">
        <!-- Start Header Area -->
        @await Html.PartialAsync("_header")
        <!-- End Header Area -->
        <div class="container">
            <div class="row height align-items-center justify-content-center">
                <div class="col-lg-10">
                    <div class="generic-banner-content">
                        <h2 class="text-white text-center">The Elements Page</h2>
                        <p class="text-white">It won’t be a bigger problem to find one video game lover in your <br> neighbor. Since the introduction of Virtual Game.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End banner Area -->
    @RenderBody()
    <!-- start footer Area -->
    @await Html.PartialAsync("_footer")
    <!-- End footer Area -->

    <script src="~/template/apps/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="~/template/apps/js/vendor/bootstrap.min.js"></script>
    <script src="~/template/apps/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/template/apps/js/jquery.magnific-popup.min.js"></script>
    <script src="~/template/apps/js/parallax.min.js"></script>
    <script src="~/template/apps/js/owl.carousel.min.js"></script>
    <script src="~/template/apps/js/jquery.sticky.js"></script>
    <script src="~/template/apps/js/hexagons.min.js"></script>
    <script src="~/template/apps/js/jquery.counterup.min.js"></script>
    <script src="~/template/apps/js/jquery.nice-select.min.js"></script>
    <script src="~/template/apps/js/waypoints.min.js"></script>
    <script src="~/template/apps/js/main.js"></script>
</body>
</html>